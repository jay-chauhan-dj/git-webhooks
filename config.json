{
  "database": {
    "tables": {
      "projects": "projects",
      "webhook_events": "webhook_events"
    }
  },
  "application": {
    "name": "Git Webhook Handler",
    "version": "2.0",
    "author": "Jay Chauhan",
    "website": "www.dj-jay.in",
    "port": 5000,
    "host": "0.0.0.0"
  },
  "logging": {
    "file": "webhook_app.log",
    "level": "INFO",
    "format": "%(asctime)s - %(levelname)s - %(message)s"
  },
  "webhook": {
    "timeout": 300,
    "max_payload_size": 10485760,
    "default_branch": "main"
  }
}